#include<bits/stdc++.h>
using namespace std;
#define int long long 
#define ld long double
int mod= 1e9+7;
#define endl "\n"
#define IOS ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

void solve()
{
    int n;
    cin>>n;
    int sum=0;
    for(int i=0;i<n;i++)
    {
        int x;
        cin>>x;
        sum^=x;
    }
    cout<<sum<<endl;
}

signed main(){
    IOS
    int t=1;
    cin>>t;
    while(t--) solve();
    return 0;
}
